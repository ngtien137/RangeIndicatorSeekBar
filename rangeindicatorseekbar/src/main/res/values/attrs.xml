<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RangeIndicatorSeekBar">
        <attr name="bar_color_background" format="color" />
        <attr name="bar_height" format="dimension" />
        <attr name="bar_corner" format="dimension" />
        <attr name="bar_color_selected" format="color" />

        <attr name="thumb_size" format="dimension" />
        <attr name="thumb_color" format="color" />
        <attr name="thumb_ripple_color" format="color" />
        <attr name="thumb_ripple_size" format="dimension" />
        <attr name="thumb_ripple_alpha" format="color" />
        <attr name="thumb_expand_touch_size" format="dimension" />

        <attr name="text_indicator_size" format="dimension" />
        <attr name="text_indicator_color" format="color" />
        <attr name="text_indicator_bottom" format="dimension" />

        <attr name="max" format="integer" />
        <attr name="min" format="integer" />
        <attr name="min_progress" format="integer" />
        <attr name="max_progress" format="integer" />
        <attr name="progress_visible_as_int" format="boolean"/>
    </declare-styleable>
</resources>